{
  "RockLib.Logging": {
    "Level": "Debug",
    "LogProviders": {
      "Type": "RockLib.Logging.ConsoleLogProvider, RockLib.Logging", 
      "Value": {
        "Formatter": {
          "Type":  "RockLib.Logging.CoreLogFormatter, RockLib.Logging" 
        } 
      } 
    } 
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:8080"
      } 
    }
  }
}
